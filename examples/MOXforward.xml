        <entry>
          <prototype>Source5</prototype>
          <number>1</number>
        </entry>       
        <entry>
          <prototype>Rx</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>Sink</prototype>
          <number>1</number>
        </entry>
      </initialfacilitylist>
      <config> <NullInst/> </config>
    </institution>
  </region>

