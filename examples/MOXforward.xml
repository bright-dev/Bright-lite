  <facility>
    <name>Sink</name>
    <config>
      <Sink>
        <in_commods>
          <val>uf</val>
        </in_commods>
        <capacity>100000000.0</capacity>
      </Sink>
    </config>
  </facility>

  <facility>
    <name>Rx</name>
    <config>
      <ReactorFacility>
        <in_commods>
            <val>freshFuel</val> 
            <val>freshFuel1</val> 
            <val>freshFuel2</val> 
            <val>freshFuel3</val> 
            <val>freshFuel4</val> 
            <val>freshFuel5</val>       
        </in_commods>
        <out_commod>uf</out_commod>
        <libraries><val>PWRMOX</val></libraries>
        <target_burnup>0.0</target_burnup>
        <nonleakage>0.9475</nonleakage>
        <core_mass>70000</core_mass>
        <generated_power>1000</generated_power>
        <batches>5</batches>
        <flux_mode>0</flux_mode>
        <burnupcalc_timestep>100</burnupcalc_timestep>
        <CR_fissile>
            <val>902250</val>
            <val>902270</val>
            <val>902290</val>
            <val>922350</val>
            <val>942380</val>
            <val>942390</val>
