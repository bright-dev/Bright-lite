            <val>952400</val>
            <val>952420</val>
            <val>952440</val>
        </CR_fissile>
      </ReactorFacility>
    </config>
  </facility>

  <region>
    <name>SingleRegion</name>
    <config> <NullRegion/> </config>
    <institution>
      <name>SingleInstitution</name>
      <initialfacilitylist>
        <entry>
          <prototype>Source</prototype>
          <number>1</number>
        </entry>  
        <entry>
          <prototype>Source1</prototype>
          <number>1</number>
        </entry>  
        <entry>
          <prototype>Source2</prototype>
